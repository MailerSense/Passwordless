{
	"name": "@passwordless-tools/shared",
	"type": "module",
	"version": "1.0.0",
	"description": "Shared utilities and types for Passwordless Tools",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"generate:client-api-ts": "openapi-ts -i client.openapi.json -o src/autogenerated/client -c @hey-api/client-fetch",
		"generate:server-api-ts": "openapi-ts -i server.openapi.json -o src/autogenerated/server -c @hey-api/client-fetch"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.10.0",
	"rules": {
		"exports": "warn",
		"types": "warn",
		"dependencies": "error",
		"files": "warn"
	},
	"ignoreExportsUsedInFile": true,
	"ignoreBinaries": ["turbo", "biome", "rslib"],
	"dependencies": {
		"@hey-api/client-fetch": "^0.11.0"
	},
	"devDependencies": {
		"@hey-api/openapi-ts": "^0.69.1"
	}
}
