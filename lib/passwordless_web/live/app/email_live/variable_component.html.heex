<div>
  <% origin = "right" %>
  <.slide_over title={@title} subtitle={@subtitle} origin={origin} max_width="lg">
    <:actions>
      <.button
        size="wide"
        type="submit"
        color="light-flat"
        label={gettext("Close")}
        phx-click={PasswordlessWeb.Components.SlideOver.hide_slide_over(origin)}
      />
    </:actions>

    <.json_block label={gettext("Example variables")} code={@example} />
  </.slide_over>
</div>
